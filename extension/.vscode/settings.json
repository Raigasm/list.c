{
  "cSpell.words": ["findfile"],
  "files.associations": {
    "list.h": "c",
    "algorithm": "cpp",
    "iterator": "c",
    "string": "c",
    "string_view": "c",
    "__locale": "c",
    "typeinfo": "c",
    "memory": "c",
    "utility": "c"
  },
  "saveAndRun": {
    "commands": [
      {
        "match": ".c",
        "cmd": "rm -rf *.o && make test && ./findfile"
      },
      {
        "match": ".h",
        "cmd": "rm -rf *.o && make test && ./findfile"
      }
    ]
  }
}
